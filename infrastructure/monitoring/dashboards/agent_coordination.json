{
  "title": "Agent Coordination",
  "uid": "neo-agent-coord",
  "schemaVersion": 39,
  "version": 1,
  "time": { "from": "now-24h", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "Agent workflow executions by status",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {"expr": "sum by(status)(rate(agent_workflow_executions_total[5m]))", "legendFormat": "{{status}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Executions by workflow and agent type",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {"expr": "sum by(workflow_name,agent_type)(rate(agent_workflow_executions_total[5m]))", "legendFormat": "{{workflow_name}}/{{agent_type}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Workflow duration (p95/avg)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum by (le)(rate(agent_workflow_duration_seconds_bucket[5m])))", "legendFormat": "p95"},
        {"expr": "sum(rate(agent_workflow_duration_seconds_sum[5m])) / sum(rate(agent_workflow_duration_seconds_count[5m]))", "legendFormat": "avg"}
      ]
    }
  ]
}




