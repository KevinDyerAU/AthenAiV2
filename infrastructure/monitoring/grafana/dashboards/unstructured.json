          "sort": 0,
          "type": "query"
        },
        {
          "current": { "selected": false, "text": "All", "value": "$__all" },
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "definition": "label_values(up{job=\"unstructured-worker\"}, instance)",
          "hide": 0,
          "includeAll": true,
          "label": "Worker Instance",
          "multi": true,
          "name": "worker_instance",
          "options": [],
          "query": { "query": "label_values(up{job=\"unstructured-worker\"}, instance)", "refId": "StandardVariableQuery" },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    },
    "annotations": {
