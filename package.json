{
  "name": "athenai-full-system",
  "version": "2.0.0",
  "description": "Full AthenAI implementation with all original agents and comprehensive functionality",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint src/ tests/ --ext .js",
    "lint:fix": "eslint src/ tests/ --ext .js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/",
    "test:e2e": "jest tests/e2e/",
    "setup": "node -e \"console.log('Use setup:win or setup:unix for your platform')\"",
    "setup:win": "powershell -ExecutionPolicy Bypass -File ./setup-knowledge-substrate.ps1",
    "setup:unix": "./setup-knowledge-substrate.sh",
    "init-db": "node -e \"console.log('Use init-db:win or init-db:unix for your platform')\"",
    "init-db:win": "powershell -ExecutionPolicy Bypass -File ./init-db.ps1",
    "init-db:unix": "./init-db.sh",
    "start-dev": "node -e \"console.log('Use start-dev:win or start-dev:unix for your platform')\"",
    "start-dev:win": "powershell -ExecutionPolicy Bypass -File ./start-dev.ps1 -Dev",
    "start-dev:unix": "./start-dev.sh",
    "check-setup": "node -e \"console.log('Use check-setup:win or check-setup:unix for your platform')\"",
    "check-setup:win": "powershell -ExecutionPolicy Bypass -File ./setup-knowledge-substrate.ps1 -CheckOnly",
    "check-setup:unix": "./setup-knowledge-substrate.sh --check-only",
    "docker:simplified": "docker-compose -f docker-compose.simplified.yml up --build -d",
    "docker:simplified:logs": "docker-compose -f docker-compose.simplified.yml logs -f",
    "docker:simplified:down": "docker-compose -f docker-compose.simplified.yml down",
    "docker:simplified:restart": "docker-compose -f docker-compose.simplified.yml restart",
    "docker:full": "docker-compose up --build -d",
    "docker:full:logs": "docker-compose logs -f",
    "docker:full:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docs:setup": "node -e \"console.log('Use docs:setup:win or docs:setup:unix for your platform')\"",
    "docs:setup:win": "powershell -ExecutionPolicy Bypass -File ./setup-document-processing.ps1",
    "docs:setup:unix": "./setup-document-processing.sh",
    "docs:worker": "python src/workers/unstructured_worker.py",
    "docs:test": "node -e \"console.log('Use docs:test:win or docs:test:unix for your platform')\"",
    "docs:test:win": "powershell -ExecutionPolicy Bypass -File ./test-document-processing.ps1",
    "docs:test:unix": "./test-document-processing.sh",
    "docs:worker:direct": "cd src/workers && python unstructured_worker.py"
  },
  "dependencies": {
    "@langchain/community": "^0.0.57",
    "@langchain/core": "^0.1.63",
    "@langchain/openai": "^0.0.34",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.37",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.15.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "puppeteer": "^19.11.1",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "socket.io-client": "^4.7.4",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinDyerAU/AthenAI.git"
  },
  "keywords": [
    "ai",
    "agents",
    "research",
    "analysis",
    "creative"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KevinDyerAU/AthenAI/issues"
  },
  "homepage": "https://github.com/KevinDyerAU/AthenAI#readme"
}
