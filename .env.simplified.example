# Application Configuration
NODE_ENV=development
PORT=3000
APP_NAME=athenai

# AI API Configuration
USE_OPENROUTER=true
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=x-ai/grok-code-fast-1
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_TEMPERATURE=0.1
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.1




# Database Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
POSTGRES_URL=postgresql://postgres:your_supabase_db_password@db.your-project.supabase.co:5432/postgres

# Neo4j Configuration
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# Redis Configuration
REDIS_URL=redis://your-redis-url:port
REDIS_PASSWORD=your_redis_password  # Optional
REDIS_USERNAME=your_redis_username  # Optional (Redis 6+)

# Langchain and Langsmith Configuration 
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=athenai
LANGCHAIN_TRACING_V2=true
LANGCHAIN_CALLBACKS_BACKGROUND=true

# Document Processing (Local Docker - No API Key Needed)
# UNSTRUCTURED_API_KEY=your_unstructured_api_key_here  # Not needed for local processing
# UNSTRUCTURED_API_URL=https://api.unstructuredapp.io/general/v0/general  # Not needed for local processing

# Document Processing Worker
UNSTRUCTURED_WORKER_URL=http://unstructured-worker:8080

# Document Processing Configuration
WORKER_CONCURRENCY=2
LOG_LEVEL=INFO
UPLOAD_DIR=./data/unstructured/input
EMBEDDING_MODEL=text-embedding-3-small

# Search APIs (Optional)
SERPAPI_API_KEY=your_serpapi_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
API_SECRET_KEY=your_api_secret_key_here

# ML Service Configuration (Optional - Enable with ENABLE_ML_SERVICE=true)
ENABLE_ML_SERVICE=false
ML_SERVICE_HOST=ml-service
ML_SERVICE_PORT=8001
MLFLOW_TRACKING_URI=http://mlflow:5000
MLFLOW_EXPERIMENT_NAME=athenai_ml
MLFLOW_ARTIFACT_ROOT=./mlruns
MLFLOW_BACKEND_STORE_URI=sqlite:///mlflow.db

# Model Configuration
ML_MODEL_CACHE_SIZE=10
ML_MODEL_LOAD_TIMEOUT=300
ML_BATCH_SIZE=32
ML_MAX_SEQUENCE_LENGTH=512

# Training Configuration
ML_TRAINING_EPOCHS=100
ML_LEARNING_RATE=0.001
ML_EARLY_STOPPING_PATIENCE=10
ML_VALIDATION_SPLIT=0.2

# Monitoring and Alerting
ML_MONITORING_ENABLED=true
ML_DRIFT_DETECTION_THRESHOLD=0.1
ML_PERFORMANCE_THRESHOLD=0.8
ML_ALERT_EMAIL=admin@example.com

# Retraining Configuration
ML_RETRAINING_ENABLED=true
ML_RETRAINING_SCHEDULE="0 2 * * 0"  # Weekly at 2 AM Sunday
ML_RETRAINING_PERFORMANCE_THRESHOLD=0.75
ML_RETRAINING_DATA_DRIFT_THRESHOLD=0.15

# Batch Processing
ML_BATCH_JOB_TIMEOUT=3600
ML_BATCH_MAX_CONCURRENT_JOBS=5
ML_BATCH_RETRY_ATTEMPTS=3

# Feature Store Configuration
ML_FEATURE_STORE_ENABLED=true
ML_FEATURE_CACHE_TTL=3600

# GPU Configuration (if available)
ML_USE_GPU=false
ML_GPU_MEMORY_FRACTION=0.8

# =============================================================================
# FEATURE FLAGS
# =============================================================================
FEATURE_ENABLE_PLANNING=true
FEATURE_ENABLE_EXECUTION=true
FEATURE_ENABLE_QA=true
FEATURE_ENABLE_COMMUNICATION=true
CONSCIOUSNESS_SUBSTRATE_ENABLED=true
INTER_AGENT_COMMUNICATION_ENABLED=true
AI_CHAT_AGENT_ENABLED=true
WORKFLOW_ORCHESTRATION_ENABLED=true
MULTI_MODEL_AI_ENABLED=true
VECTOR_SEARCH_ENABLED=true
REAL_TIME_MONITORING_ENABLED=true
ADVANCED_ANALYTICS_ENABLED=true
DYNAMIC_AGENT_CREATION_ENABLED=false
AUTO_SCALING_ENABLED=false
PREDICTIVE_ANALYTICS_ENABLED=false
QUANTUM_COMPUTING_INTEGRATION_ENABLED=false

# ML Service Feature Flags
ML_SERVICE_ENABLED=true
ML_TRAINING_ENABLED=true
ML_BATCH_PROCESSING_ENABLED=true
ML_MONITORING_ENABLED=true
ML_RETRAINING_ENABLED=true
ML_DRIFT_DETECTION_ENABLED=true
ML_FEATURE_STORE_ENABLED=true
ML_GPU_ACCELERATION_ENABLED=false
